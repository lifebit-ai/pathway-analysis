docker.enabled = true
process.container = 'cbcrg/kallisto-nf:1.1'

params {
  reads = false
  singleEnd = false
  transcriptome = false
  annotation = false
  condition = false
  fragment_len = 180
  fragment_sd = 20
  bootstrap = 100
  rmarkdown = "$baseDir/bin/DE_with_DEseq2.Rmd"
  outdir = 'results'
}

process {
  withName: gene_expression {
      container = 'lifebitai/pathway-analysis:latest'
  }
}